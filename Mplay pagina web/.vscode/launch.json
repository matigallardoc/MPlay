{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Chrome",
            "request": "launch",
            "type": "chrome",
            "url": "http://localhost:8080/index.html",
            "webRoot": "${workspaceFolder}",
            "sourceMapPathOverrides": {
                "webpack://?:*/*": "${webRoot}/*"
            }
        }
    ]
}